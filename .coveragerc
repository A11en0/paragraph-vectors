[run]
branch = True

concurrency = multiprocessing
parallel = True

source =
    paragraphvec

omit =
    *tests*
    *train.py
    *export_vectors.py

[report]
exclude_lines =
    pragma: no cover
    pass
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True
